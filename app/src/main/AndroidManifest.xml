        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.TicTacToe">

        <meta-data
            android:name="com.google.android.gms.ads.APPLICATION_ID"
            android:value="ca-app-pub-3940256099942544~3347511713"/>

        <activity android:name=".ThemesActivity"></activity>
        <activity android:name=".skins.SkinsChangerOActivity" />
        <activity android:name=".skins.SkinsCurrentActivity" />
        <activity android:name=".skins.SkinsChangerXActivity" />
        <activity android:name=".BoardMultiplayerActivity"></activity>
        <activity android:name=".PlayMultiplayerActivity" />
        <activity android:name=".PlaySingleplayerActivity" />
        <activity android:name=".SettingsActivity" />
        <activity android:name=".StatisticsActivity" />
        <activity android:name=".PlayActivity" />
        <activity android:name=".ShopActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>
